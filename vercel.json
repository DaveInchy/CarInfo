{
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/(.*(?:html|css|js|json|png|jpg|jpeg|gif|svg))$",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/([^/]+)",
      "dest": "/api/index.js"
    },
    {
      "src": "/logo/([^/]+)",
      "dest": "/api/index.js"
    }
  ]
}